{
  "manifest_version": 2,
  "name": "Appirio CES Labs",
  "short_name": "Appirio CES Labs",
  "description": "Extend the Appirio CES technology platform.",
  "version": "3.5",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "browser_action": {
    "default_icon": "appirio-icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab", 
    "cookies",
    "https://appirio.my.salesforce.com/",
    "https://appirio-community.force.com/"
  ]
}